{
  "name": "create-t3-turbo",
  "description": "Package Manager settings for SchoolConnect",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "build": "pnpm with-env turbo build",
    "start": "pnpm with-env pnpm --filter @acme/ws start",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "db:generate": "pnpm with-env turbo db:generate",
    "db:push": "pnpm with-env turbo db:push db:generate",
    "dev": "pnpm with-env turbo dev --parallel",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "lint": "turbo lint && manypkg check",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "type-check": "turbo type-check",
    "with-env": "dotenv -e .env --",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@types/prettier": "^2.7.2",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.39.0",
    "eslint-config-acme": "0.0.0",
    "prettier": "^2.8.8",
    "three": "^0.154.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4",
    "vitepress": "1.0.0-alpha.75",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "react-native-calendars@1.1293.0": "patches/react-native-calendars@1.1293.0.patch",
      "uploadthing@3.0.4": "patches/uploadthing@3.0.4.patch",
      "react-native-image-pan-zoom@2.1.12": "patches/react-native-image-pan-zoom@2.1.12.patch"
    }
  },
  "devDependencies": {
    "@types/three": "^0.152.1"
  }
}
